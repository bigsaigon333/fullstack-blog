{
  "name": "blog-webfront",
  "version": "0.0.1",
  "description": "blog-webfront",
  "main": "src/index.tsx",
  "type": "module",
  "exports": {
    "./client": {
      "default": "./dist/client/main.js"
    },
    "./server": {
      "types": "./dist/server/server.d.cts",
      "default": "./dist/server/main.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm build:prod",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:server": "pnpm build:prod --config webpack.config.server.cjs",
    "build:server:dev": "pnpm build:dev --config webpack.config.server.cjs",
    "build:client": "pnpm build:prod --config webpack.config.client.cjs",
    "watch": "webpack --watch",
    "dev": "concurrently 'pnpm dev:client' 'pnpm dev:server'",
    "dev:prod": "concurrently 'pnpm dev:client:prod' 'pnpm dev:server:prod'",
    "dev:client": "webpack --watch --node-env=development --config webpack.config.client.cjs ",
    "dev:client:prod": "webpack --watch --node-env=production --config webpack.config.client.cjs ",
    "dev:server": "webpack --watch --node-env=development --config webpack.config.server.cjs",
    "dev:server:prod": "webpack --watch --node-env=production --config webpack.config.server.cjs",
    "analyze": "webpack --mode=production --node-env=production --analyze --config webpack.config.client.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/webpack": "^5.28.1",
    "@webpack-cli/generators": "^3.0.7",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "html-webpack-plugin": "^5.5.3",
    "http-proxy-middleware": "^2.0.6",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.28",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^3.0.1",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.5",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "flowbite-react": "^0.6.0",
    "jotai": "^2.4.2",
    "ky": "^1.0.0",
    "marked": "^8.0.0",
    "query-string": "^8.1.0",
    "react": "18.3.0-canary-d803f519e-20231020",
    "react-dom": "18.3.0-canary-d803f519e-20231020",
    "react-error-boundary": "^4.0.11",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.46.1",
    "react-router-dom": "^6.15.0",
    "superjson": "^2.2.0",
    "ts-pattern": "^5.0.5",
    "use-query-params": "^2.2.1",
    "zod": "^3.22.2"
  }
}
